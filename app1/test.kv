<TesteSono0>:
    name:'testesono0'
    BoxLayout:
        orientation: 'vertical'
        canvas:
            Color:
                rgba: 1,1,1,1
            Rectangle:
                size: self.width, self.height
        Label:
            text:'Este teste serve para medir o nivel de sonolencia diurna, que atinge a muitos. No caso de caminhoneiros e ainda mais perigoso, pois podem acabar em acidentes terriveis. Sao 8 questoes e voce deve classificar a chance de cochilar relacionada a algumas atividades.  '
            color: 0,0,0,1
            size_hint: 1, None
            text_size: self.width* 0.9, None
            halign: 'center'
        BoxLayout:
            padding: 150
            Botao:
                text: 'Comecar o Teste'
                font_size: '25sp'
                on_release: app.root.current = 'testesono1'
        Label:
            text: 'Teste desenvolvido por: Johns MW. A new method for measuring daytime sleepiness: the Epworth sleepiness scale. Sleep. 1991;14:540-5. PubMed '
            color: 0,0,0,1
            size_hint: 1, None
            text_size: self.width* 0.9, None
            font_size: '10sp'
            halign: 'center'

